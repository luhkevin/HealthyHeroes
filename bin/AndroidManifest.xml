<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthyheroes"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/apple"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.healthyheroes.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.healthyheroes.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.healthyheroes.IngredientActivity"
            android:label="@string/title_activity_ingredient" >
        </activity>
        <activity
            android:name="com.example.healthyheroes.ProductActivity"
            android:label="@string/title_activity_product" >
        </activity>

        <activity
            android:name="com.example.healthyheroes.SellingActivity"
            android:label="@string/title_activity_selling" >
        </activity>
        <activity
            android:name="com.example.healthyheroes.ViewSessionActivity"
            android:label="@string/title_activity_view_session" >
        </activity>
        <activity
            android:name="com.example.healthyheroes.ViewPastSessionActivity"
            android:label="@string/title_activity_view_past_session" >
        </activity>
        <activity
            android:name="com.example.healthyheroes.PastSessionsActivity"
            android:label="@string/title_past_sessions">
        </activity>
    </application>

</manifest>
